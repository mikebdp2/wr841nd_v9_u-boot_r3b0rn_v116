# Set the toolchain path
export BOARD_TYPE=ap143
export CFG_BOARD_TYPE=$(BOARD_TYPE)

### R3B0RN gcc-4.3.3 => gcc-mips
export TOOLCHAIN=gcc-mips

### R3B0RN: specify it at "make" command
### export TOOLPREFIX=mips-linux-uclibc-

### R3B0RN build_mips/staging_dir => staging_dir
export TOOLARCH=staging_dir/usr

### export UBOOT_GCC_4_3_3_EXTRA_CFLAGS=-fPIC

### R3B0RN R3B0RN_UBOOT_EXTRA_MIPSFLAGS ###
